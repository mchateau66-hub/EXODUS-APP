generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Sat {
  id        String    @id @default(cuid())
  jti       String    @unique
  userId    String
  issuedAt  DateTime  @default(now())
  expiresAt DateTime
  usedAt    DateTime?
}
